<div class = "table-responsive">
<table class = 'table table-striped'>
<%= link_to "Back to students", students_path %>
<tr>
<th>Student_id</th>
<th>Book_name</th>
<th>Book_code</th>
<th>Date</th>
</tr>
<% @student_books.each do |book| %>
    <tr>
    <td> <%= book.student_id  %> </td>
    <td> <%= book.book_name %> </td>
    <td> <%= book.book_code     %> </td>
    <td> <%= book.date    %> </td>

    </tr>
<% end %>
</table>
</div>

